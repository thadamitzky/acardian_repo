/**
 * Expands the collectors for the agent. This file uses % as the macro delimiter
 *
 * The argument is a single collector object
 */
collectorxml(c) ::= <<  
<collector name="%c.name%" method-name="%c.methodName%" value="%c.value%" unit="%c.unit.value%">
    %fastmode(c)%
</collector>
>>

/**
 * Dumps out a fast mode line. The argument is a single Collector.fastmode element
 */
fastmode(f) ::= <<
<!-- fast mode goes here -->
>>

/**
 * Dumps out the Java method that corresponds to a named collector.
 */
collectorjava(c) ::= <<

/**
 * %c.name% Data Collector
 * 
 * @param collectionFreqInMs the collection frequency for this collector, in ms.
 */
@Override
public void %c.methodName%(long collectionFreqInMs) {
    mLogger.debug("%c.name% collector invoked");
}

>>