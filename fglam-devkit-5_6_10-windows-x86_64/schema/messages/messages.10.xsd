	<xsd:complexType name="Headers">
		<xsd:annotation>
			<xsd:documentation>
				Header elements must appear in the order they are
				defined in the schema. Specifically:
				1) an option single "poll"
				2) One or more "host"s and
				3) Zero or more "agent"s
				4) an optional single 'credentialTimestamp'
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs9:Headers"/>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AllAgentStatusResponse">
		<xsd:annotation>
			<xsd:documentation>
				Returns the status of all agents currently installed on this FglAM host. This message
				type is a performance improvement over asking for agent status from each agent individually.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamContentBlock">
				<xsd:sequence>
					<xsd:element name="agent-status" type="msgs1_3:Agent" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AllAgentStatusRequest">
		<xsd:annotation>
			<xsd:documentation>
				Single message to request all agent status info from the remote FglAM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamContentBlock"/>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:attributeGroup name="AgentControlServiceAgentQueryAttrs">
		<xsd:attribute name="remote-agent-instance-name" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-agent-id" type="msgs1_0:UUID" use="optional"/>
		<xsd:attribute name="remote-agent-type" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-agent-package" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-agent-version" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-agent-build-number" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-agent-internal-name" type="xsd:string" use="optional"/>
	</xsd:attributeGroup>

	<xsd:attributeGroup name="AgentControlServiceHostQueryAttrs">
		<xsd:attribute name="remote-host-id" type="msgs1_0:UUID" use="optional"/>
		<xsd:attribute name="remote-host-display-name" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-hostname" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-address" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-version" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-build-number" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-os-name" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-os-version" type="xsd:string" use="optional"/>
		<xsd:attribute name="remote-host-os-arch" type="xsd:string" use="optional"/>
	</xsd:attributeGroup>

	<xsd:complexType name="AgentControlServiceRemoteAgentCreate">
		<xsd:annotation>
			<xsd:documentation>
				Submitted by an Agent via the AgentControlService to create a new Agent instance
				on a remote Agent Manager
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamAgentBlock">
				<xsd:attribute name="remote-host-id" type="msgs1_0:UUID" use="required"/>
				<xsd:attribute name="remote-host-display-name" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-agent-type" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-agent-name" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-agent-internal-name" type="xsd:string" use="optional"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteAgentCreateResult">
		<xsd:annotation>
			<xsd:documentation>
				Returns the result of the create agent request
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamAgentBlock">
				<xsd:sequence>
					<xsd:element name="agent-control-service-remote-agent" type="msgs1_3:Agent" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="agent-control-service-remote-host" type="msgs6:Host" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
				<xsd:attribute name="success" type="xsd:boolean" use="required"/>
				<xsd:attribute name="errorMessage" type="xsd:string" use="optional"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteAgentSetState">
		<xsd:annotation>
			<xsd:documentation>
				Submitted by an Agent via the AgentControlService to change the state of an agent
				on a remote Agent Manager
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamAgentBlock">
				<xsd:attribute name="remote-host-id" type="msgs1_0:UUID" use="required"/>
				<xsd:attribute name="remote-host-display-name" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-agent-id" type="msgs1_0:UUID" use="required"/>
				<xsd:attribute name="remote-agent-type" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-agent-name" type="xsd:string" use="required"/>
				<xsd:attribute name="remote-state" type="msgs1_3:AgentState" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteAgentQuery">
		<xsd:annotation>
			<xsd:documentation>
				Submitted by a FglAM Agent via the AgentControlService to query the FMS about agents
				that may exist on other FglAM instances
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamAgentBlock">
				<xsd:attributeGroup ref="msgs10:AgentControlServiceAgentQueryAttrs"/>
				<xsd:attributeGroup ref="msgs10:AgentControlServiceHostQueryAttrs"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteAgentQueryResult">
		<xsd:annotation>
			<xsd:documentation>
				Returns the result of the remote agent query
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamAgentBlock">
				<xsd:sequence>
					<xsd:element name="agent-control-service-remote-agents-per-host" minOccurs="0" maxOccurs="unbounded">
						<xsd:complexType>
							<xsd:sequence minOccurs="0" maxOccurs="unbounded">
								<xsd:element name="agent-control-service-remote-agent" type="msgs1_3:Agent" minOccurs="0" maxOccurs="unbounded"/>
							</xsd:sequence>
							<xsd:attribute name="remote-host-id" type="msgs1_0:UUID"/>
							<xsd:attribute name="remote-host-display-name" type="xsd:string" use="required"/>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="agent-control-service-remote-hosts" type="msgs6:Host" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteHostQuery">
		<xsd:annotation>
			<xsd:documentation>
				Submitted by a FglAM Agent via the AgentControlService to query the FMS about agents
				that may exist on other FglAM instances
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamAgentBlock">
				<xsd:attributeGroup ref="msgs10:AgentControlServiceHostQueryAttrs"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentControlServiceRemoteHostQueryResult">
		<xsd:annotation>
			<xsd:documentation>
				Returns the result of the remote host query
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamAgentBlock">
				<xsd:sequence>
					<xsd:element name="agent-control-service-remote-host" type="msgs6:Host" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentPropertyChangeRequest">
	  <xsd:annotation>
	    <xsd:documentation>
	      Called by discovery code to change properties on an agent.
	    </xsd:documentation>
	  </xsd:annotation>
	  <xsd:complexContent>
	    <xsd:extension base="msgs10:UpstreamAgentBlock">
	      <xsd:choice>
	        <xsd:element name="primaryasp" type="msgs1_2:PrimaryASP" />
	        <xsd:element name="secondaryasp" type="msgs1_2:SecondaryASP" />
	      </xsd:choice>
	    </xsd:extension>
	  </xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DiscoveryAgentPropertyChangeRequest">
		<xsd:annotation>
			<xsd:documentation>
				Called by discovery code to change properties on an agent.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msgs10:UpstreamContentBlock">
				<xsd:choice>
					<xsd:element name="agent" type="msgs1_0:BridgeAgent" />
					<xsd:element name="primaryasp" type="msgs1_2:PrimaryASP" />
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentPropertyChange">
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamAgentBlock">
				<xsd:sequence>
					<xsd:element name="primaryasp" type="msgs1_2:PrimaryASP" minOccurs="0" />
					<xsd:element name="secondaryasp" type="msgs1_2:SecondaryASP" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AgentPropertyQueryRequest">
        <xsd:annotation>
            <xsd:documentation>
                Request issued to the server to return a set of properties for an agent.  This is
                mostly used for discovery agents that get/set properties on other agents.  An
                AgentPropertyFetchResponse will be returned by the server.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="msgs10:UpstreamContentBlock">
                <xsd:choice>
                    <xsd:element name="agent" type="msgs1_3:BridgeAgent"/>
                </xsd:choice>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

	<xsd:complexType name="AgentPropertyQueryResponse">
		<xsd:complexContent>
			<xsd:extension base="msgs10:DownstreamContentBlock">
				<xsd:sequence>
					<xsd:element name="agent" type="msgs1_0:BridgeAgent" minOccurs="1" maxOccurs="1" />
					<xsd:element name="primaryasp" type="msgs1_2:PrimaryASP" minOccurs="1" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

</xsd:schema>
